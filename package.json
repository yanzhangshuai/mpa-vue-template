{
  "name": "mpa-vue-template",
  "version": "1.0.0",
  "repository": "https://github.com/OrcasTeam/mpa-vue-template.git",
  "author": "yanzhangshuai<yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "webpack-build": "cross-env TS_NODE_PROJECT=./build/tsconfig.json webpack       --config ./build/webpack.config.ts",
    "dev": "          cross-env TS_NODE_PROJECT=./build/tsconfig.json webpack serve --config ./build/webpack.config.ts  --env env",
    "build": "        npm-run-all lint && npm-run-all webpack-build",
    "build:br": "     npm-run-all lint && cross-env WEBPACK_BUILD_COMPRESS=brotli  npm-run-all webpack-build",
    "build:gz": "     npm-run-all lint && cross-env WEBPACK_BUILD_COMPRESS=gzip    npm-run-all webpack-build",
    "deploy": "       npm-run-all build:gz && docker build -t mpa-vue-template -f ./deploy/Dockerfile .",
    "lint:js": "      eslint ./{src,test,type,build}/**/*.{js,ts,tsx,vue} --fix",
    "lint:style": "   stylelint ./src/**/*.{css,less,vue} --fix",
    "lint": "         npm-run-all --parallel lint:*",
    "test": "         jest --passWith-no-tests --config ./test/jest.config.ts --watchAll",
    "report": "       cross-env WEBPACK_REPORT=true npm-run-all webpack-build",
    "report:test": "  jest --passWith-no-tests --config ./test/jest.config.ts --coverage && open-cli ./report/test/index.html",
    "report:windi": " windicss-analysis",
    "prepare": "      husky install"
  },
  "dependencies": {
    "@mwjz/asker": "^0.0.3",
    "@mwjz/pinia-storage": "^0.0.3",
    "@pinia/plugin-debounce": "^0.1.0",
