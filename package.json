{
  "name": "mpa-vue-template",
  "version": "1.0.0",
  "repository": "https://github.com/OrcasTeam/mpa-vue-template.git",
  "author": "yanzhangshuai<yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "vue-tsc": "      vue-tsc --noEmit --skipLibCheck",
    "vite-build": "   vite build --config ./build/vite.config.ts",
    "dev": "          vite --config ./build/vite.config.ts",
    "build": "        npm-run-all vite-build",
    "build:br": "     cross-env VITE_BUILD_COMPRESS=brotli  npm-run-all vite-build",
    "build:gz": "     cross-env VITE_BUILD_COMPRESS=gzip    npm-run-all vite-build",
    "serve": "        npm-run-all vite-build && vite preview --config ./build/vite.preview.ts",
    "deploy": "       npm-run-all build:gz && docker build -t mpa-vue-template -f ./deploy/Dockerfile .",
    "lint:js": "      eslint ./{src,test,type,build}/**/*.{js,ts,tsx,vue} --fix",
    "lint:style": "   stylelint ./src/**/*.{css,less,vue} --fix",
    "lint": "         npm-run-all --parallel lint:*",
    "test": "         vitest --passWithNoTests --config ./test/vitest.config.ts",
    "report": "       cross-env VITE_REPORT=true npm-run-all vite-build",
    "report:test": "  vitest run --passWithNoTests --config ./test/vitest.config.ts --coverage  && open-cli ./report/test/index.html",
    "report:windi": " windicss-analysis",
    "prepare": "      husky install"
  },
  "dependencies": {
    "@mwjz/asker": "^0.0.3",
    "@mwjz/pinia-storage": "^0.0.3",
    "@mwjz/utils": "^0.1.8",
    "@pinia/plugin-debounce": "^0.0.2",
    "@vueuse/core": "^8.3.1",
    "axios": "^0.27.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.1",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.13",
    "query-string": "^7.1.1",
    "typescript": "^4.6.4",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "windicss": "^3.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.22.0",
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.17.6",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.4",
    "@commitlint/config-conventional": "^16.2.4",
    "@orca-studio/vite-plugin-html": "^3.2.1",
    "@rollup/plugin-babel": "^6.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.30",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/parser": "^5.21.0",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.33",
    "@vue/runtime-core": "^3.2.33",
    "@vue/test-utils": "^2.0.0-rc.21",
    "autoprefixer": "^10.4.5",
    "core-js": "^3.22.3",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.7",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "husky": "^7.0.4",
    "jsdom": "^20.0.1",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "postcss": "^8.4.13",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.6.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^3.2.3",
    "rollup-plugin-visualizer": "^5.8.3",
    "stylelint": "^14.8.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "terser": "^5.15.1",
    "ts-node": "^10.7.0",
    "vite": "^3.1.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mpa": "^1.1.4",
    "vite-plugin-windicss": "^1.8.8",
    "vitest": "^0.24.3",
    "vue-tsc": "^1.0.9",
    "windicss-analysis": "^0.3.5"
  },
  "prettier": "./standard/.prettierrc.js",
  "eslintConfig": {
    "extends": "./standard/.eslintrc.js"
  },
  "stylelint": {
    "extends": "./standard/.stylelintrc.js"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
