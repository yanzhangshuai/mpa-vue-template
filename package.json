    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/runtime-core": "^3.2.31",
    "@vue/test-utils": "^2.0.0-rc.19",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
