    "lodash-es": "^4.17.21",
    "pinia": "^2.0.14",
    "query-string": "^7.1.1",
    "typescript": "^4.7.4",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1",
    "windicss": "^3.5.6"
  },
