  "browserslist": {
    "development": [
      "chrome >= 49"
    ],
    "production": [
      "chrome >= 49"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
