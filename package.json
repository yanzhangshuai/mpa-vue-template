{
  "name": "mpa-vue-webpack",
  "version": "1.0.0",
  "repository": "https://github.com/OrcasTeam/mpa-vue-template.git",
  "author": "yanzhangshuai <yanzhangshuai@126.com>",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack serve --env env",
    "build": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack",
    "build:br": "npm-run-all lint && cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" WEBPACK_BUILD_COMPRESS=brotli  webpack",
    "build:gzip": "npm-run-all lint && cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" WEBPACK_BUILD_COMPRESS=gzip  webpack",
    "lint:js": "eslint --config .eslintrc.js  ./{src,test,type,build}/**/*.{ts,tsx,vue} --fix",
    "lint:style": "stylelint --config .stylelintrc.js ./src/**/*.{css,less,vue} --fix",
    "lint": "npm-run-all --parallel lint:*",
    "prepare": "husky install",
    "report": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\"  WEBPACK_REPORT=true webpack",
    "report:windi": "windicss-analysis"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.2.31",
    "@vueuse/core": "^8.1.2",
    "axios": "^0.26.1",
    "bucket-storage": "^0.4.6",
    "dayjs": "^1.11.0",
    "less": "^4.1.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.12",
    "query-string": "^7.1.1",
    "typescript": "^4.6.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "windicss": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.8",
    "@babel/plugin-proposal-function-bind": "^7.16.7",
    "@babel/plugin-proposal-pipeline-operator": "^7.17.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nuxt/friendly-errors-webpack-plugin": "^2.5.2",
    "@types/friendly-errors-webpack-plugin": "^0.1.4",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.22",
    "@types/qs": "^6.9.7",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.31",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.4",
    "compression-webpack-plugin": "^9.2.0",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.1.5",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "portfinder": "^1.0.28",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "postcss-loader": "^6.2.1",
    "prettier": "^2.6.0",
    "regenerator-runtime": "^0.13.9",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "terser-webpack-plugin": "^5.3.1",
    "thread-loader": "^3.0.4",
    "ts-node": "^10.7.0",
    "vue-loader": "^17.0.0",
    "webpack": "^5.70.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "webpack-merge": "^5.8.0",
    "windicss-analysis": "^0.3.5",
    "windicss-webpack-plugin": "^1.6.7"
  },
  "browserslist": {
    "development": [
      "chrome >= 49"
    ],
    "production": [
      "chrome >= 49"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": "npm run lint:js",
    "*.{css,less,vue}": "npm run lint:style"
  }
}
